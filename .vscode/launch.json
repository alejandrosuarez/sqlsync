  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug end-to-end-local",
      "type": "lldb",
      "request": "launch",
      "cargo": {
        "args": ["build", "--example=end-to-end-local"],
        "filter": {
          "name": "end-to-end-local",
          "kind": "example"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Debug end-to-end-local-net",
      "type": "lldb",
      "request": "launch",
      "cargo": {
        "args": ["build", "--example=end-to-end-local-net"],
        "filter": {
          "name": "end-to-end-local-net",
          "kind": "example"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Debug sqlsync reducer host example",
      "type": "lldb",
      "request": "launch",
      "cargo": {
        "args": ["build", "--example=host", "--features=host"],
        "filter": {
          "name": "host",
          "kind": "example"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    }
  ]
}
